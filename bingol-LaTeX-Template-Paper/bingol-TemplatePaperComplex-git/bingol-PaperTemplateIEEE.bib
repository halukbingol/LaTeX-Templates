	Journal = {Behavioral and Brain Sciences},
	Keywords = {OmerBasar,attraction,ethological theory,evolution,gender differences,life history strategies,mate selection,mating,sexual selection,similarity,social exchange},
	Mendeley-Tags = {OmerBasar,mating},
